<p id="notice"><%= notice %></p>

<h1>Group Libraries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_libraries.each do |group_library| %>
      <tr>
        <td><%= group_library.name %></td>
        <td><%= link_to 'Show', group_library %></td>
        <td><%= link_to 'Edit', edit_group_library_path(group_library) %></td>
        <td><%= link_to 'Destroy', group_library, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group Library', new_group_library_path %>
